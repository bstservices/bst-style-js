{
  "compilerOptions": {
    "module": "CommonJS",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "importHelpers": true,
    "declaration": true,
    "sourceMap": false,
    "target": "ES2015",
    "lib": ["ES2016"]
  }
}
